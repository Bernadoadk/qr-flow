# Headers pour les assets statiques
/build/*
  Cache-Control: public, max-age=31536000, immutable

# Headers pour les CSS
*.css
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: text/css

# Headers pour les JS
*.js
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: application/javascript
